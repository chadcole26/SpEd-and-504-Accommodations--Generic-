<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<psExtension 
	xmlns="http://www.powerschool.com" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.powerschool.com psextension.xsd">
    <extensionname>U_SPED504_ALERTS</extensionname>
    <extendedTable coreTable="Students" dbTableName="U_SPED504_ACCOMMODATIONS" childName="U_SPED504_ACCOMMODATIONS" comment="Accommodation info for the SpEd/504 Alerts">
        <field name="ACCOMMODATION" type="String" length="4000" comment="Special Education or 504 Accommodation"/>
        <field name="ARCHIVED" type="Double" comment="Accommodation Archived Flag"/>
        <field name="ARCHIVEDBY" type="String" length="4000" comment="Accommodation Archive By User Info"/>
        <field name="CONDITION" type="String" length="4000" comment="Accommodation Condition"/>
        <field name="CREATEDBY" type="String" length="4000" comment="Accommodation Created By User Info"/>
        <field name="ENDDATE" type="Date" comment="Accommodation End Date"/>
        <field name="FREQUENCY" type="String" length="4000" comment="Accommodation Frequency"/>
        <field name="LASTUPDATEBY" type="String" length="4000" comment="Accommodation Laste Update By User Info"/>
        <field name="LOCATION" type="String" length="4000" comment="Accommodation Location"/>
        <field name="STARTDATE" type="Date" comment="Accommodation Start Date"/>
        <field name="TYPE" type="String" length="40" comment="Accommodation Type"/>
    </extendedTable>
    <extendedTable coreTable="Students" dbTableName="U_SPED504_INFO" comment="Student services and participation information">
        <field name="END_504" type="Date" comment="End date for active 504 plan"/>
        <field name="HAS_504" type="Double" comment="Student has an active 504 plan"/>
        <field name="HAS_IEP" type="Double" comment="Student has an active IEP"/>
        <field name="IS_SPED" type="Double" comment="Student receives special education services"/>
        <field name="START_504" type="Date" comment="Start date for active 504 plan"/>
    </extendedTable>
</psExtension>
